{
  "name": "imagineer-web",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --outDir ../public",
    "build:prod": "NODE_ENV=production vite build --outDir ../public",
    "build:analyze": "NODE_ENV=production vite build --outDir ../public && (open dist/stats.html || xdg-open dist/stats.html || start dist/stats.html)",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "version:patch": "cd .. && python scripts/version.py bump --part patch",
    "version:minor": "cd .. && python scripts/version.py bump --part minor",
    "version:major": "cd .. && python scripts/version.py bump --part major",
    "version:current": "cd .. && python scripts/version.py current",
    "deploy:build": "npm run build:prod && cd .. && python scripts/version.py build-info",
    "tsc": "tsc --noEmit --project ./tsconfig.json",
    "generate:types": "cd .. && python3 scripts/generate_shared_types.py"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-focus-lock": "^2.13.6",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^6.0.11",
    "vite-plugin-compression2": "^2.3.1",
    "vitest": "^1.6.1",
    "zod": "^4.1.12"
  }
}
